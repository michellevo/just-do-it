{
  "manifest_version": 2,
  "name": "Just Do It",
  "version": "0.1",
  "permissions": ["tabs", "*://twitter.com/*"],
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": ["js/jquery-2.1.4.min.js", "js/markov.js", "js/corpus.js", "js/json2.js",  "js/content.js"]
    }
  ],

   "web_accessible_resources": [
      "images/*"
    ]
}